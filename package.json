{
  "name": "starter",
  "version": "1.0.0",
  "description": "starter",
  "main": "webpack.config.js",
  "browserslist": [
    "defaults",
    "not ie < 9",
    "last 2 versions"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack -p --config config/build.config.js --progress --colors",
    "start": "webpack --config config/dev.config.js --watch --progress --colors",
    "serve": "webpack-serve --config config/dev.config.js --content public/ --open",
    "eslint": "eslint **/*.js --ignore-pattern node_modules/ --fix",
    "stylelint": "stylelint src/**/*.scss --config .stylelintrc --syntax scss",
    "htmllint": "htmllint src/*.html || true"
  },
  "author": "romeo gogokhia",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "autoprefixer": "9.3.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-register": "6.26.0",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "4.6.0",
    "cross-env": "5.2.0",
    "css-loader": "1.0.1",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "expose-loader": "^0.7.5",
    "file-loader": "2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imagemin-webpack-plugin": "^2.1.5",
    "import-glob-loader": "^1.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.3",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "resolve-url-loader": "3.0.0",
    "sass-loader": "7.1.0",
    "script-loader": "0.7.2",
    "style-loader": "0.23.1",
    "stylelint": "^9.9.0",
    "stylelint-config-recommended": "^2.1.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "1.1.2",
    "webpack": "4.26.1",
    "webpack-cli": "3.1.2",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-merge": "4.1.4",
    "webpack-serve": "2.0.3"
  },
  "dependencies": {
    "@fancyapps/fancybox": "^3.5.6",
    "aos": "^2.3.4",
    "bootstrap": "^4.1.1",
    "hamburgers": "^1.1.3",
    "jquery": "^3.3.1",
    "jquery-doubletaptogo": "^3.0.2",
    "object-fit-images": "^3.2.4",
    "rfs": "^7.1.9",
    "scrollreveal": "^4.0.5"
  }
}
